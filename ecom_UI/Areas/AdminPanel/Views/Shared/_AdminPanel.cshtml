<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <title>FAOSM | Admin Panel</title>

    <!-- Font Awesome Icons -->

    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet" />
</head>
<body class="hold-transition sidebar-mini">
  
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fa fa-bars"></i></a>
                </li>
              
            </ul>

        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-light-primary elevation-4">
            <!-- Brand Logo -->
            <a href="index3.html" class="brand-link">
                <img src="~/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">FAOSM-Admin Panal</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="@Session["userimg"]" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        @if (User.Identity.Name != null)
                        {
                            <a href="#" class="d-block">@User.Identity.Name</a>
                            <a href="/AdminPanel/AdminLogin/logoutuser" class="d-block">Logout</a>
                        }

                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview menu-open">
                            <a href="#" class="nav-link active">
                                <i class="fa fa-dashboard"></i>
                                <p>
                                    Dashboard
                                    <i class="right fa fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/AdminPanel/Offer/offer" class="nav-link">
                                        <i class="fa fa-plus-circle nav-icon"></i>
                                        <p>Add Offers</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Offer/offerlist" class="nav-link">
                                        <i class="fa fa-list nav-icon"></i>
                                        <p>Offers List</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Product/Create_Product" class="nav-link">
                                        <i class="fa fa-plus-circle nav-icon"></i>
                                        <p>Add Product</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Product/get_product" class="nav-link">
                                        <i class="fa fa-list nav-icon"></i>
                                        <p>Product List</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Category/add_categoy" class="nav-link">
                                        <i class="fa fa-plus-circle nav-icon"></i>
                                        <p>Add Category</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Category/get_cate" class="nav-link">
                                        <i class="fa fa-list nav-icon"></i>
                                        <p>Category List</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Sub_Category/sub_cate_view" class="nav-link">
                                        <i class="fa fa-plus-circle nav-icon"></i>
                                        <p>Add Sub Category</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Sub_Category/sub_cate_list_view" class="nav-link">
                                        <i class="fa fa-list nav-icon"></i>
                                        <p>Sub Category List</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/User/adduser" class="nav-link">
                                        <i class="fa fa-user nav-icon"></i>
                                        <p>Add New User</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/User/userlist" class="nav-link">
                                        <i class="fa fa-list nav-icon"></i>
                                        <p>User List</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Role/addrole" class="nav-link">
                                        <i class="fa fa-plus-circle nav-icon"></i>
                                        <p>Add New Role</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Role/rolelist" class="nav-link">
                                        <i class="fa fa-list nav-icon"></i>
                                        <p>Role List</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Order/order" class="nav-link">
                                        <i class="fa fa-list nav-icon"></i>
                                        <p>Order</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/AdminPanel/Contact/contactlist" class="nav-link">
                                        <i class="fa fa-envelope nav-icon"></i>
                                        <p>Customer Messages List</p>
                                    </a>
                                </li>
                               
                            </ul>

                        </li>

                    </ul>
                </nav>
                
            </div>
            
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
        @RenderBody()           
        </div>       
        <!-- Main Footer -->
        <footer class="main-footer text-center">
         <strong>Copyright &copy; 2014-@DateTime.Now.Year <a href="https://adminlte.io">FAOSM</a>.</strong> All rights reserved.
        </footer>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Scripts/adminlte.min.js"></script>
</body>
</html>